{
    "name": "basketin/dashboard",
    "description": "eCommerce dashboard to help you manage your eCommerce based on laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ecommerce",
        "basketin",
        "dashboard"
    ],
    "authors": [
        {
            "name": "Karim Mohamed (Komtcho)",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "jantinnerezo/livewire-alert": "^2.2",
        "laravel/framework": "^10.0",
        "livewire/livewire": "^2.10",
        "basketin/core": "dev-develop",
        "outmart/team": "dev-main",
        "ryangjchandler/blade-tabler-icons": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Basketin\\Dashboard\\": "src/",
            "Basketin\\Modules\\": "modules/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basketin\\Dashboard\\BasketinDashboardServiceProvider",
                "Basketin\\Modules\\Catalog\\Providers\\BasketinCatalogServiceProvider",
                "Basketin\\Modules\\Sales\\Providers\\BasketinSalesServiceProvider",
                "Basketin\\Modules\\Customers\\Providers\\BasketinCustomersServiceProvider",
                "Basketin\\Modules\\Marketing\\Providers\\BasketinMarketingServiceProvider",
                "Basketin\\Modules\\System\\Providers\\BasketinSystemServiceProvider",
                "Basketin\\Modules\\System\\Configuration\\Providers\\BasketinConfigurationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}